  %title= content_for?(:title) ? "Daniel Keeton – #{yield(:title)}" : 'Daniel Keeton – Realtor'

  = stylesheet_link_tag 'http://fonts.googleapis.com/css?family=Open+Sans:400italic,700italic,400,700|Neuton:700'
  = stylesheet_link_tag 'application'
  = javascript_include_tag 'vendor/custom.modernizr'
  = javascript_include_tag 'https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false'
  = javascript_include_tag 'application'
  = csrf_meta_tag

